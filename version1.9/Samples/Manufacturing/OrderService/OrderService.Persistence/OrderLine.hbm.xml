<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="OrderService.OrderLine, OrderService" table="OrderSagaDataLines">
    <id name="Id" column="Id" type="Guid">
      <generator class="guid.comb" />
    </id>
    
    <property name="ProductId" column="ProductId" type="Guid" />
    <property name="Quantity" column="Quantity" type="float" />
    <property name="AuthorizedQuantity" column="AuthorizedQuantity" type="float" />

    <many-to-one name="Order" column="OrderSagaDataId" class="OrderService.OrderSagaData, OrderService" />

  </class>
</hibernate-mapping>
